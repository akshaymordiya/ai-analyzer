{
  "name": "ai-debug-helper",
  "version": "1.0.0",
  "main": "index.js",
  "bin": "dist/index.js",
  "pkg": {
    "scripts": "dist/index.js",
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "build"
  },
  "scripts": {
